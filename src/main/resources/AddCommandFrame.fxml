                        <Font size="15.0"/>
                    </font>
                </Label>
                <TextField fx:id="idField" layoutX="79.0" layoutY="13.0"/>
                <CheckBox fx:id="uiCommandCheckBox" layoutX="253.0" layoutY="15.0" mnemonicParsing="false" text="UI指令">
