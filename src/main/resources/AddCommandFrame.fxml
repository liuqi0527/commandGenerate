                <Label alignment="CENTER_RIGHT" contentDisplay="RIGHT" layoutX="-24.0" layoutY="3.0" prefHeight="22.0"
                       prefWidth="106.0" text="Comment:  " textAlignment="RIGHT" textOverrun="CLIP">
